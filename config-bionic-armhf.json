{
    "buildinfo": [
    {
        "prerun": [
            "mkdir -p ~/.cache/deb2appimage/AppDir/usr/bin",
            "cp ../../output/*.deb ~/.cache/deb2appimage/debs/",
            "cp ./mcpelauncher-ui-qt-armhf.startme ~/.cache/deb2appimage/AppDir/usr/bin"
        ],
        "name": "mcpelauncher",
        "version": "bionic-current",
        "deps": "DEPS_PLACEHOLDER",
        "repoarch": "armhf",
        "distrorepo": "Debian",
        "repoversion": "buster",
        "binarypath": "/usr/bin/mcpelauncher-ui-qt.startme",
        "desktoppath": "/usr/share/applications/mcpelauncher-ui-qt.desktop",
        "iconpath": "/usr/share/pixmaps/mcpelauncher-ui-qt.png",
        "usewrapper": "false",
        "postrun": [
            null
        ]
    }
    ],
    "apprunconf": [
    {
        "setpath": "true",
        "setlibpath": "true",
        "setpythonpath": "false",
        "setpythonhome": "false",
        "setpythondontwritebytecode": "false",
        "setxdgdatadirs": "false",
        "setperllib": "false",
        "setgsettingsschemadir": "false",
        "setqtpluginpath": "false",
        "exec": "/usr/bin/mcpelauncher-ui-qt.startme"
    }
    ],
    "authors": [
    {
        "type": "Author",
        "author": "MCMrARM",
        "url": "https://github.com/minecraft-linux/"
    },
    {
        "type": "Author",
        "author": "ChristopherHX",
        "url": "https://github.com/ChristopherHX/"
    }
    ]
}
