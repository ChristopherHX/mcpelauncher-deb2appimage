{
    "buildinfo": [
    {
        "prerun": [
            "mkdir -p ~/.cache/deb2appimage/AppDir/usr/bin",
            "cp ./mcpelauncher-ui-qt.startme ~/.cache/deb2appimage/AppDir/usr/bin/",
            "curl -sS http://mcpelauncher.mrarm.io/apt/ubuntu/pool/main/m/mcpelauncher-client/mcpelauncher-client_20191030-221045-eb0bc83_i386.deb -o ~/.cache/deb2appimage/debs/mcpelauncher-client.deb",
            "curl -sS http://mcpelauncher.mrarm.io/apt/ubuntu/pool/main/m/mcpelauncher-ui-qt/mcpelauncher-ui-qt_20191030-221112-98ac412_amd64.deb -o ~/.cache/deb2appimage/debs/mcpelauncher-ui-qt.deb",
            "curl -sS http://mcpelauncher.mrarm.io/apt/ubuntu/pool/main/m/msa-daemon/msa-daemon_20191030-221024-cc72517_amd64.deb -o ~/.cache/deb2appimage/debs/msa-daemon.deb",
            "curl -sS http://mcpelauncher.mrarm.io/apt/ubuntu/pool/main/m/msa-ui-qt/msa-ui-qt_20191030-221024-cc72517_amd64.deb -o ~/.cache/deb2appimage/debs/msa-ui-qt.deb"
        ],
        "name": "mcpelauncher",
        "version": "bionic-20191030",
        "deps": "libasn1-8-heimdal,libcurl4,libgssapi3-heimdal,libhcrypto4-heimdal,libheimbase1-heimdal,libheimntlm0-heimdal,libhx509-5-heimdal,libkrb5-26-heimdal,libkrb5-3,libldap-2.4-2,libnss3,libprotobuf10,libroken18-heimdal,libsasl2-2,libzip4,libwind0-heimdal",
        "repoarch": "amd64",
        "distrorepo": "Ubuntu",
        "repoversion": "bionic",
        "binarypath": "/usr/bin/mcpelauncher-ui-qt.startme",
        "desktoppath": "/usr/share/applications/mcpelauncher-ui-qt.desktop",
        "iconpath": "/usr/share/pixmaps/mcpelauncher-ui-qt.png",
        "usewrapper": "true",
        "postrun": [
            null
        ]
    }
    ],
    "apprunconf": [
    {
        "setpath": "true",
        "setlibpath": "true",
        "setpythonpath": "false",
        "setpythonhome": "false",
        "setpythondontwritebytecode": "false",
        "setxdgdatadirs": "false",
        "setperllib": "false",
        "setgsettingsschemadir": "false",
        "setqtpluginpath": "false",
        "exec": "/usr/bin/mcpelauncher-ui-qt.startme.wrapper"
    }
    ],
    "authors": [
